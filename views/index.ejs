<% include ./partials/header %>

    <div class="ui center aligned container main">
        <h1>Blog app</h1>

        <% blogs.forEach(blog => { %>
            <div>
                <h2><%= blog.title %></h2>
                <img src="<%= blog.image %>" alt="Blog Image">
                <p><%= blog.description %></p>
                <a href="/blogs/<%= blog._id %>">Read More</a>
            </div>
            <hr>     
        <% }) %>
    </div>
    
    

<% include ./partials/footer %>